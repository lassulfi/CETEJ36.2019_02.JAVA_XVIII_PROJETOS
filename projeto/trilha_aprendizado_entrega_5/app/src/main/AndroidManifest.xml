<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.utfpr.luisdanielassulfi.trilhadeaprendizado">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TechnologyListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TechnologyActivity"
            android:parentActivityName=".TechnologyListActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TechnologyListActivity" />
        </activity>
        <activity android:name=".AboutActivity"
            android:parentActivityName=".TechnologyListActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TechnologyListActivity"/>
        </activity>
    </application>

</manifest>